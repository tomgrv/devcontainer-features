{
    "core": {
        "autocrlf": "input",
        "editor": "code --wait",
        "filemode": false
    },
    "credential": {
        "useHttpPath": true
    },
    "gitflow": {
        "branch": {
            "develop": "develop",
            "master": "main"
        },
        "prefix": {
            "bugfix": "bugfix/",
            "feature": "feature/",
            "hotfix": "hotfix/",
            "release": "release/",
            "support": "support/",
            "versiontag": "v"
        }
    }
}
