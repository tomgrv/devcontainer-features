{
    "name": "@tomgrv/larasets",
    "version": "0.0.1",
    "description": "Laravel development settings and utilities",
    "files": [
        "_*.sh",
        "_*.json",
        "_*.composer.json",
        "configure-*.sh",
        "install.sh",
        "stubs/**/*"
    ],
    "bin": {
        "art": "./_art.sh",
        "dep": "./_dep.sh",
        "fwd": "./_fwd.sh",
        "run": "./_run.sh",
        "sail": "./_sail.sh",
        "secret": "./_secret.sh",
        "seed": "./_seed.sh",
        "srv": "./_srv.sh"
    },
    "scripts": {
        "configure-composer": "./configure-composer.sh",
        "configure-deployer": "./configure-deployer.sh",
        "configure-sail": "./configure-sail.sh",
        "postinstall": "./install.sh",
        "test": "echo \"Warning: no test specified\"",
        "update": "npx --yes npm-check-updates -i -u",
        "update-all": "npm run update -ws --root"
    },
    "dependencies": {
        "npm-check-updates": "^16.14.6"
    },
    "peerDependencies": {
        "@tomgrv/common-utils": "^3.17.0"
    }
}
