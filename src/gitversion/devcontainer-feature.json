{
    "name": "GitVersion feature",
    "version": "4.0.0",
    "description": "Add gitversion to your devcontainer",
    "dependsOn": {
        "ghcr.io/devcontainers/features/dotnet": "",
        "ghcr.io/tomgrv/devcontainer-features/common-utils:2": ""
    },
    "id": "gitversion",
    "installsAfter": [
        "ghcr.io/tomgrv/devcontainer-features/common-utils",
        "ghcr.io/devcontainers/features/dotnet"
    ],
    "options": {
        "version": {
            "type": "string",
            "default": "5.*",
            "description": "The version of GitVersion to install"
        }
    },
    "postCreateCommand": "configure-feature gitversion"
}